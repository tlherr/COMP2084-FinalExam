@model IEnumerable<FinalExam.Division>

@{
    ViewBag.Title = "NHL Divisions";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>NHL Divisions</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.ActionLink(item.Name, "Details", new { id = item.DivisionID })
        </td>
    </tr>
}

</table>
